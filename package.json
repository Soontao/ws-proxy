{
  "name": "@newdash/ws-proxy",
  "version": "1.0.0",
  "description": "build http proxy under websocket",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --detectOpenHandles --passWithNoTests",
    "prettier": "prettier --check --write src",
    "prebuild": "rimraf lib",
    "build": "tsc",
    "postbuild": "tsc --noEmit --p tsconfig.lib.json",
    "lint": "eslint src/** --fix",
    "release": "standard-version --no-verify && git push --follow-tags origin main",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Soontao/ws-proxy"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=7"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^12.1.4",
    "@types/debug": "^4.1.6",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.14.41",
    "@types/ws": "^7.4.6",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "axios": "^0.21.1",
    "commitlint": "^12.1.4",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "~4.3.8",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "proxy-agent": "^4.0.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.0",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@newdash/newdash": "^5.19.0",
    "debug": "^4.3.2",
    "ws": "^7.5.3"
  },
  "license": "MIT"
}
